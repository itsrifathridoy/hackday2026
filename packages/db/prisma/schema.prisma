// prisma/schema.prisma (generated by scripts/merge-prisma-schema.cjs)


// Base Prisma config shared across models

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}



// From models/user.prisma
// User model and related enums

enum Gender {
  MALE
  FEMALE
  OTHER
}

model User {
  id       Int      @id @default(autoincrement())
  name     String?
  username String   @unique
  email    String   @unique
  phone    String?  @unique
  gender   Gender?
  dob      DateTime?
}
